<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
    <Authors>Peter Waher</Authors>
    <Company>Trust Anchor Group</Company>
    <Description>TAG white-label Digital ID App. Provides authentication of legal identity, and features for wallets, smart contracts and interaction with smart devices.</Description>
    <Copyright>Copyright Â© Trust Anchor Group 2019-2021. All rights reserved.</Copyright>
    <RepositoryUrl>https://github.com/Trust-Anchor-Group/XamarinApp</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <DocumentationFile>IdApp.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Services\Data\PersonalNumbers\PersonalNumberSchemes.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Services\Data\PersonalNumbers\PersonalNumberSchemes.xml" />
  </ItemGroup>

