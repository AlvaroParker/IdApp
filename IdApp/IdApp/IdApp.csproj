    </Compile>
    <Compile Update="Pages\Identity\TransferIdentity\TransferIdentityPage.xaml.cs">
      <DependentUpon>TransferIdentityPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Pages\Main\Image\ImageView.xaml.cs">
