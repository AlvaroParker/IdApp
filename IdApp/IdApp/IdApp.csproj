    </Compile>
    <Compile Update="Pages\Main\Loading\LoadingPage.xaml.cs">
      <DependentUpon>LoadingPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Pages\Registration\ChooseAccount\ChooseAccountView.xaml.cs">
